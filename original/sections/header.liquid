{% comment %}theme-check-disable ImgLazyLoading{% endcomment %}
{%- if settings.predictive_search_enabled -%}
  <link
    rel="stylesheet"
    href="{{ 'component-predictive-search.css' | asset_url }}"
    media="print"
    onload="this.media='all'"
  >
  <link
    rel="stylesheet"
    href="{{ 'component-price.css' | asset_url }}"
    media="print"
    onload="this.media='all'"
  >
{%- endif -%}

<script src="{{ 'sticky-header.js' | asset_url }}" defer="defer"></script>

{% if template.name == 'product'
  or template.name == 'collection'
  or settings.products_show_add_to_cart
  or settings.products_show_add_to_cart_mobile
%}
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
{%- endif -%}

<script>
  window.shopUrl = '{{ shop.url }}';
  window.requestShopUrl = '{{ request.origin }}';

  window.routes = {
    cart_url: '{{ routes.cart_url }}',
    cart_add_url: '{{ routes.cart_add_url }}',
    cart_change_url: '{{ routes.cart_change_url }}',
    cart_update_url: '{{ routes.cart_update_url }}',
    predictive_search_url: '{{ routes.predictive_search_url }}',
    all_products_url: '{{ routes.all_products_collection_url }}',
    search_url: '{{ routes.search_url }}',
    root_url: '{{ routes.root_url }}',
  };

  window.accessibilityStrings = {
    shareSuccess: `{{ 'general.share.success_message' | t }}`,
  };

  window.cartStrings = {
    error: `{{ 'templates.cart.cart_error' | t }}`,
    quantityError: `{{ 'templates.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`,
  };

  window.variantStrings = {
    outOfStock: `{{ 'products.product.availability.out_of_stock' | t }}`,
    {%- if settings.show_items_in_stock -%}
    inStock: `{{ 'products.product.availability.in_stock' | t }} - {{ 'products.product.availability.number_available' | t: count: '[quantity]' }}`,
    {%- else -%}
    inStock: `{{ 'products.product.availability.in_stock' | t }}`,
    {%- endif -%}
    alwaysInStock: `{{ 'products.product.availability.always_in_stock' | t }}`,
    numberLeft: `{{ 'products.product.availability.number_left' | t: count: '[quantity]' }}`,
    numberAvailable: `{{ 'products.product.availability.number_available' | t: count: '[quantity]' }}`,
    addToCart: `{{ 'products.product.add_to_cart' | t }}`,
    unavailable: `{{ 'products.product.availability.unavailable' | t }}`,
  };

  window.themeSettings = {
    showItemsInStock: {{ settings.show_items_in_stock | json }},
    redirectToCart: {{ section.settings.redirect_to_cart | json }}
  };

  window.productsStrings = {
    facetsShowMore: `{{ 'products.facets.show_more' | t }}`,
    facetsShowLess: `{{ 'products.facets.show_less' | t }}`,
    viewFullDetails: `{{ 'products.product.view_full_details' | t }}`,
  };

  window.recentlyViewedStrings = {
    currentLanguage: `{{ localization.language.iso_code }}`,
    lessThanMinute: `{{ 'sections.recently_viewed_products.less_than_minute' | t }}`,
    justNow: `{{ 'sections.recently_viewed_products.just_now' | t }}`,
    remove: `{{ 'sections.recently_viewed_products.remove' | t }}`,
  };

  window.productCompareStrings = {
    maxProductsAlert: `{{- 'templates.collection.product_compare.max_products_alert' | t: max: '[max]' -}}`
  }

  window.productGridStrings = {
    hideFilters: `{{ 'templates.collection.hide_filters' | t }}`,
    showFilters: `{{ 'templates.collection.show_filters' | t }}`,
  };
</script>

{% liquid
  assign is_alt_layout = false
  if section.settings.header_layout == 'alt'
    assign is_alt_layout = true
  endif
%}

<style>
  .header {
  	--main-navigation-font-size: {{ section.settings.main_menu_nav_font_size }}px;
  }

  .header-heading img {
  	width: {{ section.settings.logo_width }}px;
  }

  @media (min-width: 750px) {
  	.header-heading img {
  		max-width: {{ section.settings.logo_width }}px;
  	}
  }

  @media (min-width: 990px) {
  	{% if section.settings.menu != blank and is_alt_layout == false %}
  		.head-row-main {
  			padding-top: 0;
  			padding-bottom: 0;
  		}

  		.navigation-main > .navigation-item {
  			padding-top: {{ section.settings.spacing_top }}rem;
  			padding-bottom: {{ section.settings.spacing_bottom }}rem;
  		}
  	{% else %}
  		.head-row-main {
  			padding-top: {{ section.settings.spacing_top }}rem;
  			padding-bottom: {{ section.settings.spacing_bottom }}rem;
  		}
  	{% endif %}
  }
</style>

<svg class="visually-hidden">
  <linearGradient id="rating-star-gradient-half">
  	<stop offset="50%" stop-color="var(--color-product-review-star)" />
  	<stop offset="50%" stop-color="color-mix(in srgb, rgba(var(--color-foreground)), transparent 50%)" stop-opacity="0.5" />
  </linearGradient>
</svg>

<sticky-header
  {% unless section.settings.enable_sticky_header and request.design_mode == false %}
    disabled="true"
  {% endunless %}
  {% if section.settings.sticky_header_always_on %}
    data-always-on="true"
  {% endif %}
>
  <header
    id="header"
    data-id="{{ section.id }}"
    class="header header-menu-position-{{ section.settings.menu_position }} {% if section.settings.menu == blank %}header-menu-blank{% endif %} {% if section.settings.menu_main_nav_uppercase %}header-main-nav-uppercase{% endif %} header-{{ section.settings.header_layout }}"
  >
    <div class="head-mast color-{{ section.settings.header_color_scheme }} {% if section.settings.header_remove_border %}border-none{% endif %}">
      <div class="container">
        <div class="head-row head-row-main">
          <div class="head-row-slot head-row-slot-start head-row-slot-site-branding">
            {% if section.settings.menu_mobile_position == 'start' %}
              {% render 'header-mobile-menu',
                menu: section.settings.menu,
                menu_secondary: section.settings.menu_secondary,
                menu_mobile: section.settings.menu_mobile,
                expand_on_arrow: section.settings.menu_mobile_expand_on_arrow,
                typography: section.settings.menu_font,
                blocks: section.blocks
              %}
            {% endif %}

            {% if request.page_type == 'index' %}
              <{{ section.settings.logo_homepage_tag }}
                class="header-heading h2 {% if section.settings.logo == blank %}header-heading-textual{% endif %}"
              >
            {% else %}
              <span
                class="header-heading h2 {% if section.settings.logo == blank %}header-heading-textual{% endif %}"
              >
            {% endif %}
            <a href="{{ routes.root_url }}">
              {%- if section.settings.logo -%}
                {%- assign image_size = section.settings.logo_width -%}
                {%- assign image_size2 = section.settings.logo_width | times: 2 -%}
                <img
                  src="{{ section.settings.logo | image_url: width: image_size }}"
                  srcset="{{ section.settings.logo | image_url: width: image_size }} 1x, {{ section.settings.logo | image_url: width: image_size2 }} 2x"
                  alt="{{ section.settings.logo.alt | default: shop.name | escape }}"
                  width="{{ section.settings.logo.width }}"
                  height="{{ section.settings.logo.height }}"
                >
              {%- else -%}
                {{ shop.name }}
              {%- endif -%}
            </a>
            {%- if request.page_type == 'index' -%}
              </{{ section.settings.logo_homepage_tag }}>
            {% else %}
              </span>
            {% endif %}

            {% if section.settings.menu_secondary != blank %}
              <div class="head-row-slot-mega-menu">
                {% render 'mega-menu',
                  menu: section.settings.menu_secondary,
                  color_scheme: section.settings.navigation_submenu_color_scheme,
                  button_label: section.settings.menu_secondary_button_label,
                  side_item_image: section.settings.menu_secondary_item_side_item_image,
                  side_item_title: section.settings.menu_secondary_item_side_item_title,
                  side_item_text: section.settings.menu_secondary_item_side_item_text,
                  side_item_url: section.settings.menu_secondary_item_side_item_url
                %}
              </div>
            {% endif %}
          </div>

          {%- if section.settings.menu != blank and is_alt_layout == false -%}
            <div class="head-row-slot head-row-slot-mid head-row-slot-main-nav">
              <nav class="nav-main typography-{{ section.settings.menu_font }}">
                {%- liquid
                  assign align_right = false
                  if section.settings.menu_position == 'right'
                    assign align_right = true
                  endif
                -%}
                {%- render 'header-main-menu',
                  menu: section.settings.menu,
                  blocks: section.blocks,
                  align_right: align_right
                -%}
              </nav>
            </div>
          {%- endif -%}

          {% if is_alt_layout %}
            <div class="head-row-slot head-row-slot-mid head-row-slot-search">
              {% render 'header-search',
                popular_terms: section.settings.search_popular_terms,
                is_dropdown_modal: false
              %}
            </div>
          {% endif %}

          <div class="head-row-slot head-row-slot-end head-row-slot-items">
            {%- if section.settings.enable_language_selector
              and localization.available_languages.size > 1
            -%}
              <div class="head-slot-item head-slot-item-dropdown head-slot-item-lang">
                {%- render 'dropdown-language',
                  classname: 'dropdown-right dropdown-down',
                  id: 'Header'
                -%}
              </div>
            {%- endif -%}

            {%- if section.settings.enable_country_selector
              and localization.available_countries.size > 1
            -%}
              <div class="head-slot-item head-slot-item-dropdown head-slot-item-region">
                {%- render 'dropdown-region',
                  classname: 'dropdown-right dropdown-down',
                  id: 'Header'
                -%}
              </div>
            {%- endif -%}

            {% if is_alt_layout == false %}
              <div class="head-slot-item head-slot-item-global-search">
                {% render 'header-search', is_dropdown_modal: true %}
              </div>
            {% endif %}

            {%- if shop.customer_accounts_enabled -%}
              <a
                href="{%- if customer -%}{{ routes.account_url }}{%- else -%}{{ routes.account_login_url }}{%- endif -%}"
                class="head-slot-item head-slot-item-account"
              >
                <span class="visually-hidden">
                  {%- if customer -%}
                    {{ 'customer.account_fallback' | t }}
                  {%- else -%}
                    {{ 'customer.log_in' | t }}
                  {%- endif -%}
                </span>
                {% render 'icons', icon: 'user-alt' %}

                {% if customer %}
                  {% if section.settings.account_link_text_logged_in != blank %}
                    <span class="head-slot-nav-link-label">
                      {{ section.settings.account_link_text_logged_in }}
                    </span>
                  {% endif %}
                {% else %}
                  {% if section.settings.account_link_text_logged_out != blank %}
                    <span class="head-slot-nav-link-label">
                      {{ section.settings.account_link_text_logged_out }}
                    </span>
                  {% endif %}
                {% endif %}
              </a>
            {%- endif -%}

            {% unless settings.catalog_enabled %}
              <div class="head-slot-item">
                {% render 'header-mini-cart' %}
              </div>
            {% endunless %}

            {% if section.settings.menu_mobile_position == 'end' %}
              {% render 'header-mobile-menu',
                menu: section.settings.menu,
                menu_secondary: section.settings.menu_secondary,
                menu_mobile: section.settings.menu_mobile,
                expand_on_arrow: section.settings.menu_mobile_expand_on_arrow,
                typography: section.settings.menu_font,
                blocks: section.blocks
              %}
            {% endif %}
          </div>
        </div>
      </div>
    </div>

    {%- if is_alt_layout and section.settings.menu != blank -%}
      <div class="navigation-wrapper typography-{{ section.settings.menu_font }} color-{{ section.settings.navigation_color_scheme }}">
        <div class="container">
          <nav class="nav-main">
            {%- render 'header-main-menu',
              menu: section.settings.menu,
              blocks: section.blocks,
              menu_item_highlight_text: section.settings.menu_item_highlight_text,
              menu_item_highlight_color: section.settings.menu_item_highlight_color,
              align_right: false
            -%}
          </nav>
        </div>
      </div>
    {%- endif -%}
  </header>
</sticky-header>

{% schema %}
